<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:SampleWebView.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SampleWebView.Views.MainView" >

	<Grid RowDefinitions="50,*">
		
		<Button Content="Baidu" Command="{Binding BaiduCommand}"/>
		
		<WebView Grid.Row="1" x:Name="PART_WebView" Url="https://www.bilibili.com" BorderThickness="1"></WebView>

	</Grid>
</UserControl>
